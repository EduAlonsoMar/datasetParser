<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ComboBox?>
<BorderPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <center>
        <VBox alignment="CENTER" spacing="10">
            <Label text="PARSER">
                <font>
                    <Font size="20"/>
                </font>
            </Label>
            <HBox alignment="BASELINE_LEFT">
                <Label text="Open file">
                    <font>
                        <Font size="10"/>
                    </font>
                </Label>
                <TextField>

                </TextField>
                <Button text="Explore" onAction="#fileToParseSelected"/>
            </HBox>

            <VBox alignment="BASELINE_LEFT" spacing="5">
                <Label text="Data sets labeled">
                    <font>
                        <Font size="15"/>
                    </font>
                </Label>
                <HBox>
                    <ComboBox fx:id="datasetLabeledList">

                    </ComboBox>
                    <Button text="Show Results" onAction="#selectedDataSetLabeledForResults" />
                </HBox>
                <Label text="Data sets not labeled">
                    <font>
                        <Font size="15"/>
                    </font>
                </Label>
                <HBox>
                    <ComboBox fx:id="datasetNotLabeledList">

                    </ComboBox>
                    <Button text="Show Results" onAction="#selectedDataSetNotLabeledForResults" />
                </HBox>
            </VBox>

        </VBox>
    </center>
</BorderPane>
